
<%- include layouts/header.ejs %>

        <div class="page-data">
            
            <form method="post" action="/customers/add" enctype="multipart/form-data">
                <table cellpadding="11" class="form">
                <tr>
                    <td class="label" align="center">Name</td><td>: <input type="text" name="name" required></td>
                 </tr>
                <tr>
                    <td class="label" align="center">Email</td><td>: <input type="text" name="email" required></td>
                 </tr>
                  <tr>
                    <td class="label" align="center">Phone</td><td>: <input type="text" name="phone" required></td>
                 </tr>
                <tr>
                    <td class="label" align="center">Images</td><td>: <input type="file" name="imgUploader" required /></td>
                 </tr>
 <tr>
                    <td class="label"align="center">Address</td><td valign="middle">: <textarea name="address" required cols="30" rows="5"></textarea></td>
                </tr>
                 <tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input type="submit" name="submit" value="Save">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                    </td>
                 </tr>
                </table>
            </form>
            
        </div>
        
<%- include layouts/footer.ejs %>
